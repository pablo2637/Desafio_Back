{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "./controllers",
            "./configs",
            "./helpers",
            "./middlewares",
            "./routes",
            "./models"
        ],
        "exclude": [
            "node_modules/|docs"
        ]
    },
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true
    }
}